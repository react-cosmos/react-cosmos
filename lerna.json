{
  "version": "6.0.0-alpha.11",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org",
      "allowBranch": "main",
      "ignoreChanges": [
        "packages/*/README.md",
        "packages/*/*.js",
        "packages/*/bin/*.js"
      ]
    }
  }
}
